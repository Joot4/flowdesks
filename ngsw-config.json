{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(svg|png|jpg|jpeg|webp|woff2)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "supabase-api",
      "urls": ["https://*.supabase.co/rest/v1/**", "https://*.supabase.co/auth/v1/**", "https://*.supabase.co/rpc/**"],
      "cacheConfig": {
        "maxSize": 30,
        "maxAge": "1h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    }
  ]
}
